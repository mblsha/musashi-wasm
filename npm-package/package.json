{
  "name": "musashi-wasm",
  "version": "0.1.0",
  "description": "WebAssembly port of the Musashi M68000 CPU emulator with optional Perfetto tracing support",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/musashi-loader.js",
      "require": "./dist/musashi-loader.js"
    },
    "./perfetto": {
      "import": "./dist/musashi-perfetto-loader.js",
      "require": "./dist/musashi-perfetto-loader.js"
    }
  },
  "keywords": [
    "m68k",
    "m68000",
    "motorola",
    "emulator",
    "cpu",
    "wasm",
    "webassembly",
    "perfetto",
    "tracing"
  ],
  "author": "Musashi Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mblsha/Musashi.git"
  },
  "bugs": {
    "url": "https://github.com/mblsha/Musashi/issues"
  },
  "homepage": "https://github.com/mblsha/Musashi#readme"
}

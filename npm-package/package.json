{
  "name": "musashi-wasm",
  "version": "0.1.0",
  "description": "WebAssembly port of the Musashi M68000 CPU emulator with optional Perfetto tracing support",
  "type": "module",
  "files": [
    "dist",
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    },
    "./perfetto": {
      "import": "./lib/perfetto.mjs",
      "types": "./lib/perfetto.d.ts"
    }
  },
  "types": "./lib/index.d.ts",
  "main": "./lib/index.mjs",
  "keywords": [
    "m68k",
    "m68000",
    "motorola",
    "emulator",
    "cpu",
    "wasm",
    "webassembly",
    "perfetto",
    "tracing"
  ],
  "author": "Musashi Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mblsha/Musashi.git"
  },
  "bugs": {
    "url": "https://github.com/mblsha/Musashi/issues"
  },
  "homepage": "https://github.com/mblsha/Musashi#readme"
  ,
  "scripts": {
    "build": "node ./scripts/generate-wrapper.js",
    "prepare": "npm run build"
  }
}
